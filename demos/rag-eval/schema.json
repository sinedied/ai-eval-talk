{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "text": {
      "type": "string",
      "description": "The answer or response text."
    },
    "product": {
      "type": "string",
      "enum": [
        "Contoso Trek",
        "Contoso Dash",
        "Contoso Glide",
        "Contoso Flex",
        "Contoso Chill"
      ],
      "description": "The name of the product, must be one of the predefined values."
    }
  },
  "required": ["text", "product"],
  "additionalProperties": false
}
